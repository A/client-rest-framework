{
  "name": "client-rest-framework",
  "version": "0.3.0",
  "description": "REST API framework to construct domain API repositories",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "repository": "https://github.com/A/client-rest-framework",
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@types/lodash.merge": "^4.6.7",
    "axios": "^1.6.0",
    "debug": "4.4.1",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@jest/globals": "^29.4.3",
    "@types/debug": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "codecov": "^3.5.0",
    "cspell": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^3.1.0",
    "jest": "^29.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^9.0.0",
    "tsafe": "^1.4.3",
    "typescript": "^4.0.2"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  }
}
